# public class FizzBuzz {
    public void run()
    { int a = 1;
        while (a <= 100) {
            System.out.println("a is" +a+ "but is less than or equal to 100");
            a = a + 1;
        if ( a / 3 <= 33 && a / 3 >= 1) {
            System.out.println("Fizz");
        }
        if ( a / 5 <= 20 && a / 5 >= 1) {
            System.out.println("Buzz");
        }
        if ( a / 15 <= 6 && a / 15 >= 1) {
            System.out.println("FizzBuzz");
        }  
        }
    }
}
